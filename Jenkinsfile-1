pipeline {
    agent {
        docker {
            //image 'python:3.9-alpine'
            label "linux"
            }
    }
    stages {
        stage('Build') {
            steps {
                sh 'docker buld -t math_game .'
            }
        }
        stage('Run') {
            steps {
                sh 'docker run -d math_game'
            }
        }
    }
}
